<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:weather="http://www.example.org/weather#">

    <owl:Ontology rdf:about="http://www.example.org/weather"/>

    <owl:Class rdf:about="weather:WeatherCondition"/>

    <owl:DatatypeProperty rdf:about="weather:temperature">
        <rdfs:domain rdf:resource="weather:WeatherCondition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="weather:condition">
        <rdfs:domain rdf:resource="weather:WeatherCondition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

</rdf:RDF>
